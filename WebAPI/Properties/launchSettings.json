{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/WebAPI",
      "sslPort": 0,
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:45342/WebAPI",
      "sslPort": 44368
